{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "node": "current"
        }
      }
    ],
    "@babel/preset-react",
    "@babel/preset-flow"
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-proposal-object-rest-spread",
    [
      "babel-plugin-styled-components",
      {
        "fileName": false
      }
    ],
    ["module-resolver", {
      "root": ["./client"],
      "alias": {
        "config": "./client/config",
        "features": "./client/features",
        "types": "./client/types",
        "style": "./client/style"
      },
      "cwd": "packagejson"
    }]
  ]
}
